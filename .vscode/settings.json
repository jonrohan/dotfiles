{
    "editor.fontSize": 17,
    "editor.cursorStyle": "block",
    "editor.tabSize": 2,
    "editor.multiCursorModifier": "ctrlCmd",
    "files.autoSave": "onFocusChange",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "editor.accessibilitySupport": "off",
    "editor.roundedSelection": false,
    "editor.scrollBeyondLastColumn": 10,
    "editor.scrollBeyondLastLine": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#2979FF",
        "list.activeSelectionForeground": "#2979FF",
        "list.inactiveSelectionForeground": "#2979FF",
        "list.highlightForeground": "#2979FF",
        "scrollbarSlider.activeBackground": "#2979FF50",
        "editorSuggestWidget.highlightForeground": "#2979FF",
        "textLink.foreground": "#2979FF",
        "progressBar.background": "#2979FF",
        "pickerGroup.foreground": "#2979FF",
        "tab.activeBorder": "#2979FF",
        "notificationLink.foreground": "#2979FF",
        "editorWidget.resizeBorder": "#2979FF",
        "editorWidget.border": "#2979FF",
        "settings.modifiedItemIndicator": "#2979FF",
        "settings.modifiedItemForeground": "#2979FF",
        "settings.headerForeground": "#2979FF",
        "panelTitle.activeBorder": "#2979FF",
        "breadcrumb.activeSelectionForeground": "#2979FF",
        "menu.selectionForeground": "#2979FF",
        "menubar.selectionForeground": "#2979FF"
    },
    "material-icon-theme.folders.theme": "classic",
    "material-icon-theme.activeIconPack": "react",
    "material-icon-theme.hidesExplorerArrows": true,
    "debug.console.fontSize": 15,
    "markdown.preview.fontSize": 15,
    "terminal.integrated.fontSize": 15,
    "liveshare.featureSet": "insiders",
    "editor.fontFamily": "\"Fira Code\"",
    "vsicons.dontShowNewVersionMessage": true,
    "window.autoDetectColorScheme": true,
    "workbench.preferredDarkColorTheme": "GitHub Dark Default",
    "workbench.preferredLightColorTheme": "Night Owl Light (No Italics)",
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": false,
    "gitlens.codeLens.enabled": false,
    "ruby.rubocop.suppressRubocopWarnings": true,
    "ruby.rubocop.useBundler": true,
    "editor.codeActionsOnSave": null,
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "auto-close-tag.activationOnLanguage": [

        "xml",
        "php",
        "blade",
        "ejs",
        "jinja",
        "plaintext",
        "markdown",
        "vue",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml",
        "HTML (EEx)",
        "HTML (Eex)",
        "plist"
    ],
    "vsicons.presets.hideFolders": true,
    "explorer.openEditors.sortOrder": "alphabetical",
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.inlineSuggest.enabled": true,
    "editor.smartSelect.selectLeadingAndTrailingWhitespace": false,
    "files.trimTrailingWhitespace": true,
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true
    },
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "extensions.ignoreRecommendations": true,
    "diffEditor.renderSideBySide": false,
    "projectManager.git.baseFolders": [
        "~/Code/"
    ],
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh"
        },
        "fish": {
            "path": "fish"
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        },
        "zsh (2)": {
            "path": "/usr/bin/zsh"
        }
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "stylelint.reportNeedlessDisables": true,
    "stylelint.validate": [
      "css",
      "postcss",
      "scss"
    ],
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "githubPullRequests.createOnPublishBranch": "never",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "solargraph.useBundler": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.editor.showTabs": "single",
    "terminal.integrated.scrollback": 3000,
    "githubPullRequests.pullBranch": "never",
    "editor.stickyScroll.enabled": true,
    "workbench.iconTheme": "vs-minimal",
    "comments.openView": "never",
    "workbench.colorTheme": "Night Owl Light (No Italics)",
    "workbench.tree.renderIndentGuides": "none",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "terminal.explorerKind": "external",
    "scm.defaultViewMode": "tree",
    "explorer.fileNesting.patterns": {
        "Gemfile": "Gemfile.lock"
    },
    "editor.detectIndentation": false,
    "github.codespaces.showPerformanceExplorer": true,
    "github.codespaces.connectionType": "any",
    "github.copilot.enable": {
      "*": true,
      "yaml": false,
      "plaintext": true,
      "markdown": false,
      "scminput": false
    },
    "editor.minimap.autohide": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.scale": 2,
    "files.trimFinalNewlines": true,
    "stylelint.snippet": [
      "css",
      "less",
      "postcss",
      "scss"
    ],
    "workbench.activityBar.location": "hidden",
    "workbench.editor.empty.hint": "hidden"
}
